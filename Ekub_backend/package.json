    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "consola": "^3.2.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "express-validator": "^7.0.1",
    "firebase-admin": "^12.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-geocoder": "^4.4.1",
    "node-schedule": "^2.1.1",
