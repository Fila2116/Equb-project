{
  "name": "equb-backend",
  "version": "1.0.0",
  "description": "Backend for hagerigna equb app",
  "main": "/dist/index.js",
  "scripts": {
    "build": "tsc",
    "prisma:push": "npx prisma db push",
    "prisma:generate": "npx prisma generate",
    "prisma:reset": "npx prisma migrate reset",
    "serve": "node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "seed:branch": "ts-node ./src/seeders/branch/create.seeder.ts",
    "seed:role": "ts-node ./src/seeders/role/create.seeder.ts",
    "seed:equbtype": "ts-node ./src/seeders/equb-type/create.seeder.ts",
    "seed:equbcategory": "ts-node ./src/seeders/equb-category/create.seeder.ts",
    "seed:staff": "ts-node ./src/seeders/staff/create.seeder.ts",
    "seed:update-staff": "ts-node ./src/seeders/staff/update.seeder.ts",
    "seed:create": "ts-node ./src/seeders/create.seeder.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/ondemand22/equb/equb-backend.git"
  },
  "keywords": [
    "equb",
    "api",
    "express",
    "typescript",
    "postgres"
  ],
  "author": "Natnael Feleke",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/ondemand22/equb/equb-backend/issues"
  },
